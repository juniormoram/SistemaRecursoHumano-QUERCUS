@model RHQuercus.Aguinaldo

@{
    ViewBag.Title = "Delete";
}


<h3>Desea eliminar este aguinaldo?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Colaborador
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Persona.NombrePers)
            @Html.DisplayFor(model => model.Persona.Apellidos)
        </dd>
        <hr />
        <dt>
            Periodo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Anno)
        </dd>
        <dt>
            Monto del aguinaldo
        </dt>

        <dd>
            ₡ @Html.DisplayFor(model => model.MontoAguinaldo)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        </div>
    }
</div>
