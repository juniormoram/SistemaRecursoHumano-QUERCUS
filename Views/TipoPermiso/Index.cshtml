@model IEnumerable<RHQuercus.TipoPermiso>

@{
    ViewBag.Title = "Index";
}

<h2>Tipos de permisos laborales</h2>


<table class="table">
    <tr>
        @*<th>
            #
        </th>*@
        <th>
            Permiso laboral
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        @*<td>
            @Html.DisplayFor(modelItem => item.IDTipoPermi)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.ActionLink("Actualizar tipo", "Edit", new { id = item.IDTipoPermi }, new { @class = "btn btn-info" }) |
        </td>
    </tr>
}

</table>
