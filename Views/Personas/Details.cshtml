@model RHQuercus.Persona

@{
    ViewBag.Title = "Details";
}

<h2>Información del colaborador</h2>

<div>    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NombrePers)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Apellidos)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Apellidos)
        </dd>
        <dt>
            Ocupación
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Ocupacion1.NombreOcu)
        </dd>
        <hr />
        <dt>
            Dirección
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Direccion)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Celular)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Celular)
        </dd>

        <dt>
            Correo electrónico
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Correo)
        </dd>
        <hr />
        @*<dt>
           Estado del colaborador
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Estado)
        </dd>*@

