        </dt>

        <dd>
            @Html.DisplayFor(model => model.FechaIngreso)
        </dd>

        <dt>
            Cant. vacaciones
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CantVacaciones)
        </dd>
        <hr />
        <dt>
            Nombre parentesco
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NombreContacto)
        </dd>

        <dt>
            Parentesco
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ParentescoContacto)
        </dd>

        <dt>
            Celular parentesco
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CelularContacto)
        </dd>

